class Solution(object):
    def searchInsert(self, nums, target):
        n = len(nums)
        
        if target < nums[0]:
            return 0
        
        if target > nums[n-1]:
            return n
        
        for i in range(n-1):
            if target == nums[i]:
                return i
            if target > nums[i] and target <= nums[i+1]:
                return i+1
        
        return 0
